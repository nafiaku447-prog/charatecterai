{
    "version": 2,
    "builds": [
        {
            "src": "setup-server.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/setup-server.js"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ]
}